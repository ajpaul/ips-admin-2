{
  "name": "angular-webpack",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "typings install",
    "build": "webpack --config config/webpack.dev",
    "start": "webpack-dev-server --config config/webpack.dev.js --progress --watch --display-error-details --display-cached --content-base src/",
    "start:dev": "npm start -- --inline",
    "start:dev:hot": "npm run start:dev -- --hot",
    "start:mockServer": "node launchMockServer.js",
    "start:mockServer:reset": "node launchMockServer.js reset",
    "webdriver-manager": "webdriver-manager",
    "protractor": "protractor",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "pree2e": "npm run webdriver:update -- --standalone",
    "e2e": "npm run protractor",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "test": "karma start karma.config.js"
  },
  "repository": "",
  "description": "",
  "license": "MIT",
